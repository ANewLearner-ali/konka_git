QCheckBox{
	font-family: PingFangSC-Regular;
	font-size: 14px;
	color: rgba(0,0,0,0.65);
	letter-spacing: 0;
	text-align: left;
	line-height: 22px;
}
QCheckBox::indicator{
	background: #FFFFFF;
	border: 1px solid #40A9FF;
	border-radius: 2px;
	border-radius: 2px;
}
QCheckBox::indicator:checked{
    background-color: #40A9FF;
	image: url(:/src/qss/checkbox.png);
}